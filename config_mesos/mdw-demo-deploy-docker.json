{
  "id": "/mdw/mdw-demo-docker",
  "cpus": 1.0,
  "mem": 1000.0,
  "disk": 1000.0,
  "instances": 1,
  "fetch": [
    {
        "uri": "https://raw.githubusercontent.com/CenturyLinkCloud/mdw-demo/master/config_mesos/mdw.yaml"
    },
    {
        "uri": "https://raw.githubusercontent.com/CenturyLinkCloud/mdw-demo/master/config_mesos/access.yaml"
    },
    {
        "uri": "https://raw.githubusercontent.com/CenturyLinkCloud/mdw-demo/master/config_mesos/seed_users.json"
    }
  ],
  "container": {
    "docker": {
      "image": "mdwcore/mdw:6.1.05",
      "parameters": []
    },
    "type": "DOCKER",
    "volumes": [],
    "portMappings": [
      {
        "containerPort": 8080,
        "servicePort": 31200,
        "protocol": "tcp",
        "name": null,
        "labels": null
      },
      {
        "containerPort": 8009,
        "protocol": "tcp",
        "name": null,
        "labels": null
      },
      {
        "containerPort": 3308,
        "servicePort": 31201,
        "protocol": "tcp",
        "name": null,
        "labels": null
      }
    ]
  },
  "networks": [
    {
      "mode": "container/bridge"
    }
  ],
  "env": {
    "JAVA_OPTS": "-Duser.home=/usr/local/tomcat/temp -Dmdw.runtime.env=dev -Dmdw.config.location=$MESOS_SANDBOX -Xmx512m"
  },
  "labels": {},
  "healthChecks": [
    {
      "protocol": "HTTP",
      "path": "/mdw/Services/AppSummary",
      "portIndex": 0,
      "gracePeriodSeconds": 300,
      "intervalSeconds": 60,
      "timeoutSeconds": 20,
      "maxConsecutiveFailures": 3
    }
  ],
  "constraints": [["hostname", "LIKE", "mesosagentidc05-test.idc1.level3.com"]]
}